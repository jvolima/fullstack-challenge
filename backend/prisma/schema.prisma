// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Article {
  id Int @id
  title String
  url String
  imageUrl String
  newsSite String
  summary String
  publishedAt String
  featured Boolean
}
